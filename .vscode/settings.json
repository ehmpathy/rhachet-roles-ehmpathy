{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": false
  },
  "cSpell.words": [
    "artee",
    "assemblylang",
    "Bhrain",
    "codegen",
    "coderef",
    "cortal",
    "datetime",
    "Enanswer",
    "endialogue",
    "Enquestion",
    "ponderage",
    "rhachet",
    "Schedulable",
    "stitchee"
  ],
  "terraform.languageServer.enable": false,
  "eslint.enable": true,
  "yaml.validate": false,
  "cSpell.enabled": true
}
